{
  "expo": {
    "name": "d3-project",
    "slug": "d3-project",
    "version": "2.0.0",
    "orientation": "landscape",
    "icon": "./assets/icone.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joaolop3s.d3project"
    },
    "android": {
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icone.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.joaolop3s.d3project"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
